package com.acorn.web;

import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.GetMapping;

public class Ex01 {


	@GetMapping("/model1")
	public String method1(Model model) {
		model.addAttribute("message1","spring");
		model.addAttribute("message2","spring2");
		model.addAttribute("message3","spring3");
		return "model1";

	}
}
